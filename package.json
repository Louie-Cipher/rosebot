{
    "name": "rosebot",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "start:dev": "ts-node src/index.ts",
        "start:node": "cross-env NODE_ENV=prod node dist/index.js",
        "build": "tsc",
        "clearDist": "rimraf dist/*",
        "clearSrc": "rimraf src/*",
        "start": "npm run clearDist && npm run build && npm run start:node",
        "start:prod": "npm install && npm run build && npm run clearSrc && npm run start:node"
    },
    "keywords": [
        "discord",
        "bot",
        "discord-bot"
    ],
    "author": "louie-cipher",
    "license": "MIT",
    "engines": {
        "node": ">=16.6.0"
    },
    "dependencies": {
        "@discordjs/builders": "^0.13.0",
        "canvas": "^2.9.1",
        "chalk": "^4.1.2",
        "discord.js": "^13.7.0",
        "dotenv": "^16.0.1",
        "glob": "^8.0.3",
        "jimp": "^0.16.1",
        "moment": "^2.29.3",
        "sqlite3": "^5.0.8",
        "tmi.js": "^1.8.5",
        "typeorm": "^0.3.6"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/node": "^17.0.35",
        "@types/tmi.js": "^1.8.1",
        "cross-env": "^7.0.3",
        "rimraf": "^3.0.2",
        "ts-node": "^10.8.0",
        "typescript": "^4.6.4"
    }
}
